{
  "name": "geoviz",
  "version": "0.9.15",
  "description": "d3-based JavaScript library for thematic maps",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build",
    "docs": "jsdoc --configure jsdoc.json --verbose",
    "deploy-docs": "jsdoc --configure jsdoc.json --verbose && gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geoviz.git"
  },
  "keywords": [
    "dataviz",
    "geoviz",
    "maps",
    "cartography",
    "d3"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geoviz/issues"
  },
  "homepage": "https://github.com/neocarto/geoviz#readme",
  "dependencies": {
