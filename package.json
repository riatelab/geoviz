{
  "name": "geoviz",
  "version": "0.4.5",
  "description": "d3-based javascript library for thematic maps",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build",
    "docs": "documentation build src/** -f html --github -o docs --config documentation.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geoviz.git"
  },
  "keywords": [
    "maps",
    "cartography",
    "d3"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geoviz/issues"
  },
  "homepage": "https://github.com/neocarto/geoviz#readme",
  "dependencies": {
    "d3-force": "^3.0.0",
    "d3-format": "^3.1.0",
    "d3-geo": "^3.1.0",
    "d3-geo-projection": "^4.0.0",
    "d3-geo-scale-bar": "^1.2.5",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-tile": "^1.0.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "dicopal": "^0.5.0",
    "statsbreaks": "^1.0.6",
    "versor": "^0.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "docdash": "^2.0.1",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  }
}
